{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ignore-path ../.gitignore **/*.{js,vue,jsx}",
    "lint:fix": "eslint --ignore-path ../.gitignore **/*.{js,vue,jsx} --fix",
    "lint:style": "stylelint --ignore-path ../.gitignore **/*.scss && stylelint --ignore-path ../.gitignore **/*.{vue,html} --custom-syntax postcss-html",
